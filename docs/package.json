    "@docusaurus/module-type-aliases": "3.8.1",
    "@docusaurus/types": "3.8.1",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "prettier": "^3.5.3"
  },
  "packageManager": "yarn@4.10.3"
