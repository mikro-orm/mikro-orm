{
  "name": "mikro-orm",
  "version": "3.0.0-rc.6",
  "description": "TypeScript ORM for Node.js based on Data Mapper, Unit of Work and Identity Map patterns. Supports MongoDB, MySQL, PostgreSQL and SQLite databases as well as usage with vanilla JavaScript.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mikro-orm/mikro-orm.git"
  },
  "keywords": [
    "orm",
    "mongo",
    "mongodb",
    "mysql",
    "mariadb",
    "postgresql",
    "sqlite",
    "sqlite3",
    "ts",
    "typescript",
    "js",
    "javascript",
    "entity",
    "ddd",
    "mikro-orm",
    "unit-of-work",
    "data-mapper",
    "identity-map"
  ],
  "author": "Martin Ad√°mek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikro-orm/mikro-orm/issues"
  },
  "homepage": "https://mikro-orm.io",
  "scripts": {
    "build": "rimraf dist && tsc",
    "pretest": "node tests/pre-test",
    "test": "node --expose-gc ./node_modules/.bin/jest --runInBand --logHeapUsage",
    "posttest": "node tests/post-test",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 2",
    "coverage": "rimraf temp tests/generated-entities && yarn test --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "tslint -p ."
  },
  "bin": {
    "mikro-orm": "./dist/cli.js"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/tests/.*\\.(test|spec))\\.(js|ts)$",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "lib/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "lib/cli.ts"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "tslint --fix",
      "git add"
    ]
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "pinVersions": false
  },
  "dependencies": {
    "acorn-loose": "^7.0.0",
    "acorn-walk": "^7.0.0",
    "chalk": "^3.0.0",
    "cli-highlight": "^2.1.4",
    "cli-table3": "^0.5.1",
    "clone": "^2.1.0",
    "fast-deep-equal": "^3.0.0",
    "fs-extra": "^8.0.0",
    "globby": "^10.0.0",
    "knex": "^0.20.8",
    "reflect-metadata": "^0.1.13",
    "ts-morph": "^4.3.3",
    "umzug": "^2.2.0",
    "uuid": "^3.3.3",
    "yargonaut": "^1.1.4",
    "yargs": "^15.0.0"
  },
  "peerDependencies": {
    "mariadb": "^2.1.0",
    "mongodb": "^3.3.4",
    "mysql2": "^2.0.0",
    "pg": "^7.10.0",
    "sqlite3": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "mariadb": {
      "optional": true
    },
    "mongodb": {
      "optional": true
    },
    "mysql2": {
      "optional": true
    },
    "pg": {
      "optional": true
    },
    "sqlite3": {
      "optional": true
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/clone": "^0.1.30",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^24.0.23",
    "@types/mongodb": "^3.3.11",
    "@types/mysql2": "types/mysql2",
    "@types/node": "^12.12.12",
    "@types/pg": "^7.11.2",
    "@types/umzug": "^2.2.2",
    "@types/uuid": "^3.4.6",
    "@types/webpack-env": "^1.14.1",
    "conditional-type-checks": "^1.0.4",
    "conventional-changelog-angular": "^5.0.6",
    "conventional-changelog-cli": "^2.0.28",
    "coveralls": "^3.0.8",
    "husky": "^4.0.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.3",
    "mariadb": "^2.1.3",
    "mongodb": "^3.3.4",
    "mysql2": "^2.0.1",
    "pg": "^7.14.0",
    "rimraf": "^3.0.0",
    "semantic-release": "^16.0.0",
    "sqlite3": "^4.1.0",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.2",
    "tslint": "^5.20.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-lines-between-class-members": "^1.3.6",
    "typescript": "3.6.4"
  }
}
